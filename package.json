{
    "name": "docker-node-express-mongodb-boilerplate",
    "description": "A demo app illustrating the usage of express, mongoose, mysql, pug and other modules in nodejs",
    "keywords": [
        "docker",
        "express",
        "mongodb",
        "mysql"
    ],
    "version": "1.0.0",
    "private": false,
    "author": "Saifullah Khan <saifkhan912@gmail.com> (https://github.com/saifkhan192/)",
    "repository": {
        "type": "git",
        "url": "https://github.com/saifkhan192/docker-node-express-mongodb-boilerplate.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=10.11.0"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development nodemon server.js",
        "debug": "cross-env NODE_ENV=development nodemon --inspect server.js",
        "debug2": "cross-env NODE_ENV=development node --inspect=0.0.0.0 server.js",
        "test": "cross-env NODE_ENV=test babel-tape-runner test/test-*.js",
        "pretest": "eslint .",
        "prod": "node server.js --env production",
        "debug3": "set NODE_ENV=development && nodemon --inspect server.js",
        "debug4": "set NODE_ENV=development && node --inspect=0.0.0.0 server.js",
        "debug5": "export NODE_ENV=development && nodemon --inspect server.js",
        "debug6": "export NODE_ENV=development && node --inspect=0.0.0.0 server.js"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "bcrypt": "^5.0.0",
        "body-parser": "~1.19.0",
        "co": "~4.6.0",
        "compression": "~1.7.4",
        "connect-flash": "~0.1.1",
        "connect-mongo": "~3.2.0",
        "cookie-parser": "~1.4.4",
        "cors": "~2.8.5",
        "cross-env": "~6.0.3",
        "dotenv": "~8.2.0",
        "express": "~4.17.1",
        "express-session": "~1.17.0",
        "fastest-validator": "^1.9.0",
        "js-yaml": "^3.14.1",
        "mongoose": "~5.8.4",
        "morgan": "~1.9.1",
        "pug": "~2.0.4",
        "winston": "~3.2.1"
    },
    "devDependencies": {
        "babel-eslint": "~10.0.3",
        "babel-tape-runner": "~3.0.0",
        "eslint": "~6.8.0",
        "eslint-config-prettier": "~6.9.0",
        "eslint-plugin-prettier": "~3.1.2",
        "nodemon": "2.0.2",
        "npm-check-updates": "~4.0.1",
        "prettier": "~1.19.1",
        "supertest": "*",
        "tape": "~4.12.1"
    }
}