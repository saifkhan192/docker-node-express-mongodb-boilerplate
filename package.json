{
    "name": "docker-node-express-mongodb-boilerplate",
    "description": "A demo app illustrating the usage of express, mongoose, mysql, pug and other modules in nodejs",
    "keywords": [
        "docker",
        "express",
        "mongodb",
        "mysql"
    ],
    "version": "1.0.0",
    "private": false,
    "author": "Saifullah Khan <saifkhan912@gmail.com> (https://github.com/saifkhan192/)",
    "repository": {
        "type": "git",
        "url": "https://github.com/saifkhan192/docker-node-express-mongodb-boilerplate.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=10.11.0"
    },
    "scripts": {
        "dev": "export NODE_ENV=development && nodemon --inspect=0.0.0.0 --ignore node_modules/ server.js",
        "debug": "export NODE_ENV=development && node --inspect=0.0.0.0 server.js",
        "prod": "node server.js --env production",
        "test": "export NODE_ENV=test && tape src/tests/test-*.js",
        "eslint": "eslint ./src/*.js",
        "eslint:fix": "eslint --fix ./src/*.js"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "co": "^4.6.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "~4.17.1",
        "fastest-validator": "^1.9.0",
        "js-yaml": "^3.14.1",
        "mongoose": "~5.11.8",
        "morgan": "^1.10.0",
        "pg": "^8.5.1",
        "pug": "^3.0.0",
        "sequelize": "^6.3.5",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-prettier": "^3.3.0",
        "nodemon": "^2.0.6",
        "npm-check-updates": "^10.2.5",
        "prettier": "^2.2.1",
        "supertest": "*",
        "tape": "^5.0.1"
    }
}