{
    "configurations": [
        {
            "name": "Docker: Attach to Node",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9229,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "skipFiles": [
                "${workspaceFolder}/node_modules/**",
            ],
            "sourceMapPathOverrides": {
                "${workspaceFolder}/*": "app/*"
              }
        }
    ]
  }